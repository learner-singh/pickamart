{
  "name": "pickamart",
  "version": "1.0.0",
  "description": "eCommerce application built with Mern Stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learner-singh/pickamart.git"
  },
  "author": "Kishan Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/learner-singh/pickamart/issues"
  },
  "homepage": "https://github.com/learner-singh/pickamart#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "nodemon": "^3.1.10"
  }
}
